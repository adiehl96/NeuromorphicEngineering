    </visualElement>
    <visualElement>
      <elementName>Testcase</elementName>
      <elementAttributes>
        <entry>
          <string>Testdata</string>
          <testData>
            <dataString>clk clr Select in_0 in_1 Out

############################ Vertex 0 to every other Vertex 
loop(t,9)
  C 1 0 (t) 0 0 # reset line

  loop(a,8)
    C 0 0 (t) 0 0
  end loop

  C 0 0 (t) 0 (t)
end loop
</dataString>
          </testData>
        </entry>
        <entry>
          <string>generic</string>
          <string>number_of_inputs := 9;</string>
        </entry>
